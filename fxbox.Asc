POSITION:
If SETTING$(16)=_FALSE$ or P_LENGTH=0 or _MENUDO Then Return 
If _MODE=4
   PPSTEP=1
   PPY=HDR_H
   PPH=(WVE_H/4)
   Gosub GFXSET
   PPCOUNT=1
   Repeat 
      If ADVPOS#(PPCOUNT)>0
         XP(PPCOUNT)=Int((OFFSET(PPCOUNT)+1)/ADVPOS#(PPCOUNT))-1
         If XD>0
            XP(PPCOUNT)=XP(PPCOUNT)-(ST/ADVPOS#(PPCOUNT))
         End If 
         If Not RENDERING
            If XP2(PPCOUNT)<>XP(PPCOUNT)
               Ink _WHITE
               Gr Writing %10
               Draw XP(PPCOUNT),PPY To XP(PPCOUNT),PPY+PPH
               If XP2(PPCOUNT)>-1
                  Ink _WHITE
                  Gr Writing %10
                  Draw XP2(PPCOUNT),PPY To XP2(PPCOUNT),PPY+PPH
               End If 
               XP2(PPCOUNT)=XP(PPCOUNT)
            End If 
         End If 
      End If 
      PPY=PPY+PPH
      Inc PPCOUNT
   Until PPCOUNT>4
   Gr Writing %1
   Gosub GFXRESTORE
Else 
   If ADVPOS#(1)>0 and OFFSET(1)>0
      If STA>0
         XP(1)=Int(OFFSET(1)/ADVPOS#(1))
         If XD>0
            If ST>0
               XP(1)=Int(XP(1)-(ST/ADVPOS#(1)))
            End If 
         Else 
            If STA>0 and RST>0
               XP(1)=Int(XP(1)-(STA/ADVPOS#(1))+(RST/ADVPOS#(1)))
            End If 
         End If 
      Else 
         If(OFFSET(1)-ST)>0
            XP(1)=Int(((OFFSET(1)-ST)/ADVPOS#(1)))
         Else 
            XP(1)=0
         End If 
      End If 
      If Not RENDERING
         If XP(1)<>XP2(1)
            Gr Writing %10
            Draw XP(1),POS_T To XP(1),WVE_H+HDR_H-1
            If XP2(1)>-1
               Draw XP2(1),HDR_H To XP2(1),WVE_H+HDR_H-1
            End If 
            Gr Writing %1
            XP2(1)=XP(1)
         End If 
      End If 
   End If 
End If 
Return 
'  
SYNC:
If CON#<=0 Then Return 
PPCOUNT=PBANK-7
If PPCOUNT<0
   PPCOUNT=1
Else If PPCOUNT>_MODE
   PPCOUNT=_MODE
End If 
POS#=TIME(PPCOUNT)/CON#
If _TEXT=False
   BEAT=Int(POS#/(SCR_W/_TIMEDIV#))
   If SBEAT<>BEAT
      SBEAT=BEAT
      CL=Instr(SEQ$,_NEWLINE$)
      If CL>0
         T$=Left$(SEQ$,CL-1)
         SEQ$=Mid$(SEQ$,CL+1)
         If T$<>_$
            MACRO=True
            Repeat 
               CL=Instr(T$,_COMMA$)
               If CL>0
                  D$=Mid$(T$,CL+1)
                  T$=Upper$(Left$(T$,CL-1))
                  _TOAST$=_TOAST$+T$
                  Gosub _COMMAND
                  T$=D$
               End If 
            Until CL=0
            MACRO=False
         Else 
            _TOAST$=_MINUS$
         End If 
         Gosub T0AST
      End If 
   End If 
End If 
If Not RENDERING
   If SETTING$(18)<>_FALSE$
      BEAT=Int(((POS#+1)/(SCR_W/(_TIMEDIV#/2)))+1)
      BEAT=Btst(0,BEAT)
      If BEAT and METRO
         Gosub GNOME
         METRO=False
      Else If BEAT=False and METRO=False
         Gosub GNOMEC
         METRO=True
      End If 
   End If 
End If 
If NETWORK and BSD
   BEAT=Int(POS#/13.3333)
   If Btst(0,BEAT)
      If PBEAT<>BEAT
         Add PKTCOUNT,4
         Loke Varptr(MSG$)+20,PKTCOUNT
         Gosub SOCK_SENDSTRING
         PBEAT=BEAT
      End If 
   End If 
End If 
Return 
'
POS_CAL:
P_LENGTH=Length(PBANK)
If P_LENGTH=0 Then Return 
P_START=Start(PBANK)
If _MODE<4
   If _MODE=2
      P_LENGTH=P_LENGTH/2
   Else If _MODE=3
      P_LENGTH=P_LENGTH/4
   End If 
   ADVPOS#(1)=(L-BSIZE)/SCR_W#
Else 
   PAC=1
   Repeat 
      ADVPOS#(PAC)=(D_LENGTH(PAC)-BSIZE-ST-FIN)/SCR_W#
      Inc PAC
   Until PAC>4
End If 
Return 
'
NUDGEPOS:
If Not BGPLYING Then Return 
Gosub GFXSET
If _MODE=4
   NPSTEP=1
   NPY=HDR_H
   NPH=(WVE_H/4)
   NPCOUNT=1
   Repeat 
      Gr Writing %10
      Ink _WHITE
      Draw XP(NPCOUNT),NPY To XP(NPCOUNT),NPY+NPH
      NPY=NPY+NPH
      Inc NPCOUNT
   Until NPCOUNT>4
Else 
   Gr Writing %10
   Draw XP2(1),POS_T To XP2(1),WVE_H+HDR_H-1
End If 
Gr Writing %1
Gosub GFXRESTORE
Return 
'
POS_SET:
RNG_T=1
If _MODE=4
   Gosub MPOS
   WVE_D=WVE_H/4
   If CY>HDR_H and CY<=HDR_H+WVE_D
      B=1
   Else If CY>HDR_H+WVE_D and CY<=HDR_H+WVE_D+WVE_D
      B=2
   Else If CY>HDR_H+WVE_D+WVE_D and CY<=HDR_H+WVE_D+WVE_D+WVE_D
      B=3
   Else If CY>HDR_H+WVE_D+WVE_D+WVE_D and CY<=HDR_H+WVE_H
      B=4
   End If 
   Gosub BANK_SET
   If PB<>B
      Gosub NUDGEPOS
   End If 
   Gosub SETBANK
   If Length(B+7)=0
      Gosub CLEARPLAYBUFFER
   End If 
End If 
Return 
