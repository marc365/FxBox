INITMENU:
_BAR$=_SPACE$+String$(_MINUS$,14)+_SPACE$
_WHITESPACE$="    "
BANKMENU$="BANKMENU"
M0DEMENU$="M0DEMENU"
RATEMENU$="RATEMENU"
_MACRO$="MACRO"
_BGRECORD$=" Record Output   "
If Not Fn A1200 Then _OSCILLOSCOPE$=" Oscilloscope    " Else _OSCILLOSCOPE$=_NULL$
M2=1 : I=11
Repeat 
   MENB$(M2)=" F"+Str$(M2)-_SPACE$+" -"+Space$(I)
   Inc M2
   If M2=10 Then Dec I
Until M2>10
Return 
'
DRWMENU:
Paper _SHADOW : Pen _BLACK
If Not _MENUDO
   _MENUDO=True
   _M1SEL=0 : _M2SEL=0 : _M3SEL=0
   Gosub NUDGEPOS
   Gosub RANGEPXCLEAR
End If 
If _MENUSUB
   Restore 0+Val(Str$(M1)-_SPACE$+Str$(M2-2)-_SPACE$)
   Read S$
   If CY<8 or CX<MENX(_M1SEL-1) or(CX>MENX(_M1SEL-1)-8 and CX<MENX(_M1SEL-1)+Text Length(S$)-40 and CY/8<>_M2SEL)
      _MENUSUB=False
      _END=0
      Gosub CLRSUBMENU
      Goto MDO
   End If 
   If _MENUSUB
      _M3SEL=CY/8-_M2SEL+1
      If _M3SEL>_M3 or CX<SUBX or CX>SUBX+SUBW
         _M3SEL=0
      End If 
      Restore 0+Val(Str$(M1)-_SPACE$+Str$(_M2SEL)-_SPACE$)
      Read S$
      _L2=(MENX(M1-1)/8+Text Length(S$)/8)-4
      SUBX=(_L2*8)
      SUBY=(_M2SEL*8)-3
      M3=1
      Repeat 
         Restore 0+Val(Str$(_M1SEL)-_SPACE$+Str$(_M2SEL)-_SPACE$+Str$(M3)-_SPACE$)
         Read S$
         Locate _L2,_M2SEL+M3-1
         If _M3SEL=M3
            Paper _PANE
         Else 
            Paper _SHADOW
         End If 
         Pen _BLACK
         Print S$
         If Len(S$)>1
            SUBW=Text Length(S$)
         End If 
         Inc M3
      Until S$=_NULL$
      Gosub TESTMOVE
   End If 
   SUBH=(M3-1)*8
