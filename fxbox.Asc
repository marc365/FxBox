'***************************************************************************** 
'***************************************************************************** 
'  
'   ::::::-.  :::::::..    :::.     .::    .   .:: 
'   ;;,   `';,;;;;``;;;;   ;;`;;    ';;,  ;;  ;;;' 
'   `[[     [[\[[[,/[[['  ,[[ '[[,   '[[, [[, [['  
'    $$,    $$"$$$$$$c   c$$$cc$$$c    Y$c$$$c$P   
'    888_,o8P',888b "88bo 888   888,    "88"888    
'   "MMMMP"`  "MMMM   "W" YMM   ""`      "M "M"    
'
'
'
_DRAW:
Screen 1
Cls _PANE,0,0 To SCR_W,WVE_H
Gosub DRWINFO
Ink _SHADOW
WVE_D=WVE_H/2
Draw 0,WVE_D To SCR_W,WVE_D
If M0DE>1
   WVE_D=WVE_H/4
   DCY=WVE_H/2
   Draw 0,WVE_D To SCR_W,WVE_D
   Draw 0,WVE_D+DCY To SCR_W,WVE_D+DCY
End If 
If M0DE>2
   WVE_D=WVE_H/4
   DCY=-(WVE_H/8)
   DA=0
   Repeat 
      Draw 0,WVE_D+DCY To SCR_W,WVE_D+DCY
      DCY=DCY+WVE_D
      Inc DA
   Until DA>3
End If 
If M0DE<>4 and Length(5)=0
   Goto _SHOWWAVE
End If 
Ink _WHITE
If M0DE=1
   WVE_D=WVE_H/2
   ADV#(1)=(Length(5)-ST-FIN)/SCR_W#
   D_START(1)=Start(5)+ST
   CN=Peek(D_START(1)+1)
   DA=0
   If CN>127
      CN=CN-256
   End If 
   Gr Locate 0,CN+WVE_D
   Repeat 
      CN=Int(DA*ADV#(1))
      CN=Peek(D_START(1)+CN)
      If CN>127
         CN=CN-256
      End If 
      Draw To DA,CN+WVE_D
      Inc DA : Inc DA
   Until DA>SCR_W
Else If M0DE=2
   WVE_D=WVE_H/2
   D_LENGTH(1)=Length(5)/2
   ADV#(1)=(D_LENGTH(1)-ST-FIN)/SCR_W#
   D_START(1)=Start(5)+ST
   D_START(2)=D_START(1)+D_LENGTH(1)
   DC=1
   DCY=WVE_D/2
   Repeat 
      CN=(Peek(D_START(DC)+1)*128)/512
      DA=1
      If CN>31
         CN=CN-64
      End If 
      Gr Locate 0,CN+DCY
      Repeat 
         CN=Int(DA*ADV#(1))
         CN=(Peek(D_START(DC)+CN)*128)/512
         If CN>31
            CN=CN-64
         End If 
         Draw To DA,CN+DCY
         Inc DA : Inc DA
      Until DA>SCR_W
      Inc DC
      DCY=DCY+WVE_D
   Until DC>2
Else If M0DE=3
   WVE_D=WVE_H/4
   D_LENGTH(1)=Length(5)/4
   ADV#(1)=(D_LENGTH(1)-ST-FIN)/SCR_W#
   DC=1
   DCY=WVE_D/2
   Repeat 
      D_START(DC)=Start(5)+ST+DCY
      DCY=DCY+D_LENGTH(1)
      Inc DC
   Until DC>4
   DC=1
   DCY=WVE_D/2
   Repeat 
      CN=(Peek(D_START(DC)+1)*128)/1024
      DA=1
      If CN>15
         CN=CN-32
      End If 
      Gr Locate 0,CN+DCY
      Repeat 
         CN=Int(DA*ADV#(1))
         CN=(Peek(D_START(DC)+CN)*128)/1024
         If CN>15
            CN=CN-32
         End If 
         Draw To DA,CN+DCY
         Inc DA : Inc DA
      Until DA>SCR_W
      Inc DC
      DCY=DCY+WVE_D
   Until DC>4
Else If M0DE=4
   WVE_D=WVE_H/4
   DC=1
   Repeat 
      D_LENGTH(DC)=Length(DC+7)
      If D_LENGTH(DC)>0
         ADV#(DC)=(D_LENGTH(DC)-ST-FIN)/SCR_W#
         D_START(DC)=Start(DC+7)+ST
      Else 
         ADV#(DC)=0
         D_START(DC)=0
      End If 
      Inc DC
   Until DC>4
   DC=1
   DCY=WVE_D/2
   Repeat 
      If D_LENGTH(DC)>0
         CN=(Peek(D_START(DC)+1)*128)/1024
         DA=1
         If CN>15
            CN=CN-32
         End If 
         Gr Locate 0,CN+DCY
         Repeat 
            CN=Int(DA*ADV#(DC))
            CN=D_START(DC)+CN
            If CN>D_START(DC)
               CN=(Peek(CN)*128)/1024
               If CN>15
                  CN=CN-32
               End If 
               Draw To DA,CN+DCY
            Else 
               Draw 0,DCY To SCR_W,DCY
               DA=SCR_W
               Exit 
            End If 
            Inc DA : Inc DA
         Until DA>SCR_W
      End If 
      DCY=DCY+WVE_D
      Inc DC
   Until DC>4
End If 
_SHOWWAVE:
Screen Copy 1,0,0,SCR_W,WVE_H To 0,0,HDR_H
Screen 0
Return 
