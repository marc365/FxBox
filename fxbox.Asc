If _AMIVER>4
   Gosub _POSITION
   If TGL=1
      Gosub DRW_LEV
   Else 
      Gosub _PROGBAR
      Scroll 6
   End If 
Else 
   If TGL=1
      If OSCILLO=False
         Gosub _POSITION
         Gosub _PROGBAR
         Scroll 6
      End If 
   Else 
      Gosub DRW_LEV
   End If 
End If 
If _UPDATETIME
   Gosub DRWTIME
End If 
If P_LENGTH>0
   If M0DE=4
      SBCOUNT=1
      Repeat 
         OFFSET(SBCOUNT)=OFFSET(SBCOUNT)+BSIZE
         Inc SBCOUNT
      Until SBCOUNT>4
   Else 
      OFFSET(1)=OFFSET(1)+BSIZE
   End If 
   If _DEBUG
      If Sam Swapped(1) or Sam Swapped(2)
         _TOAST$="u" : Rem Buffer underrun
      End If 
   End If 
   If BGREC
      BBUFF=0 : SBCOUNT=1
      On M0DE Goto MD1_REC,MD2_REC,MD34_REC,MD34_REC
      MD1_REC:
      Ssave 3,P_BUF To P_BUF+BSIZE
      Goto CNT_REC
      MD2_REC:
      Repeat 
         Ssave SBCOUNT+2,P_BUF+BBUFF To P_BUF+BBUFF+BSIZE
         BBUFF=BBUFF+BSIZE
         Inc SBCOUNT
      Until SBCOUNT>2
      Goto CNT_REC
