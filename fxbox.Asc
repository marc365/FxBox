               FPOS=FI-1
               If FPOS>FTOTAL-16
                  FPOS=FTOTAL-16
               End If 
               Gosub DRWDR
               Exit 
            End If 
         End If 
      Next FI
   End If 
   If PZN<>Z
      Gosub DRWDR : Rem Filename Rollover Rollout
      PZN=Z
   End If 
   If M=1
