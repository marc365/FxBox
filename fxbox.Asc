_MIX:
Gosub _MIXP
Gosub _SHOWALL
Return 
'
_MIXP:
Gosub SET_PBANK
If FX_LENGTH<2
   Gosub FLSHSCR
   Return 
End If 
FX_LENGTH2=Length(6)
If FX_LENGTH2=0
   Gosub _NOCOPY
   Return 
End If 
FX_START2=Start(6)
Dec FX_LENGTH2
FX2=0
Repeat 
   CN=Peek(FX)
   CN2=Peek(FX_START2+FX2)
   Gosub UNSIGN
   If CN<0 and CN2<0
      CN=(CN+CN2)-((CN*CN2)/-127)
   Else If CN>0 and CN2>0
      CN=(CN+CN2)-((CN*CN2)/127)
   Else 
      CN=CN+CN2
   End If 
   Inc FX2
   If FX2>FX_LENGTH2
      FX2=0
      If Not MIXL00P
         FX=FXE-1
      End If 
   End If 
   Gosub SIGN
   Poke FX,CN
   Inc FX
Until FX>FXE
Return 
'
_ADD:
Gosub SET_PBANK
If FX_LENGTH<2
   Gosub FLSHSCR
   Return 
End If 
FX_LENGTH2=Length(6)
If FX_LENGTH2=0
   Gosub _NOCOPY
   Return 
End If 
FX_START2=Start(6)
FX2=0
Repeat 
   CN=Peek(FX)
   CN2=Peek(FX_START2+FX2)
   Gosub UNSIGN
   CN=CN+CN2
   FX2=FX2+1
   If FX2>FX_LENGTH2
      If MIXL00P
         FX2=0
      Else 
         FX=FXE
         Exit 
      End If 
   End If 
   Gosub SIGN
   Poke FX,CN
   Inc FX
Until FX>FXE
Return 
'
_SUBTRACT:
Gosub SET_PBANK
If FX_LENGTH<2
   Gosub FLSHSCR
   Return 
End If 
FX_LENGTH2=Length(6)
If FX_LENGTH2=0
   Gosub _NOCOPY
   Return 
End If 
FX_START2=Start(6)
FX2=0
Repeat 
   CN=Peek(FX)
   CN2=Peek(FX_START2+FX2)
   Gosub UNSIGN
   CN=CN-CN2
   FX2=FX2+1
   If FX2>FX_LENGTH2
      If MIXL00P
         FX2=0
      Else 
         FX=FXE
         Exit 
      End If 
   End If 
   Gosub SIGN
   Poke FX,CN
   Inc FX
Until FX>FXE
Return 
'
_MODULATE:
Gosub SET_PBANK
If FX_LENGTH<2
   Gosub FLSHSCR
   Return 
End If 
FX_LENGTH2=Length(6)
If FX_LENGTH2=0
   Gosub _NOCOPY
   Return 
End If 
FX_START2=Start(6)
FX2=0
Repeat 
   CN=Peek(FX)
   CN2=Peek(FX_START2+FX2)
   Gosub UNSIGN
   CN#=CN2
   CN#=CN#/127
   CN=CN*CN#
   FX2=FX2+1
   If FX2>FX_LENGTH2
      If MIXL00P
         FX2=0
      Else 
         FX=FXE
         Exit 
      End If 
   End If 
   Gosub SIGN
   Poke FX,CN
   Inc FX
Until FX>FXE
Return 
'
_INTERLEAVE:
Gosub SET_PBANK
If FX_LENGTH<2
   Gosub FLSHSCR
   Return 
End If 
FX_LENGTH2=Length(6)
_SELF=False
If FX_LENGTH2=0
   RST=Length(PBANK)/2
   RL=Length(PBANK)/2
   XR=SCR_W/2 : XR2=SCR_W
