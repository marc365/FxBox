If REALTIME and _AMIVER>4
   SBCOUNT=0
   Repeat 
      If Not CHAIN$(SBCOUNT)=""
         _CHANNEL=1
         If M0DE=2
            Repeat 
               Gosub CHAIN$(SBCOUNT)
               P_SWP=P_SWP+BSIZE
               Inc _CHANNEL
            Until _CHANNEL>2
         Else If M0DE=3 or(M0DE=4 and Key Shift)
            Repeat 
               Gosub CHAIN$(SBCOUNT)
               P_SWP=P_SWP+BSIZE
               Inc _CHANNEL
            Until _CHANNEL>4
         Else 
            If M0DE=4
               P_SWP=P_SWP+(BSIZE*(B-1))
            End If 
            _CHANNEL=1
            Gosub CHAIN$(SBCOUNT)
         End If 
      End If 
      Inc SBCOUNT
   Until SBCOUNT>CH
End If 
