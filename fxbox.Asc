If _AMIVER>4
   If M0DE=1 or M0DE=4
      '  
      '********************      
      If OSCILLO and Not _DIRECT
         Gosub OSC_BCK
         Repeat 
            OSC_X=PI*2
            OSC_X=Peek(OSC_START+OSC_X)
            If OSC_X>127
               OSC_X=OSC_X-256
            End If 
            Plot PI+(SCR_W/2)-128,(OSC_X/2)+77,_GREEN
            Inc PI : Inc PI
         Until PI>255
      End If 
   Else 
      '  
      '********************
      If OSCILLO and Not _DIRECT
         Gosub OSC_BCK
         Repeat 
            OSC_X=Peek(OSC_START+PI)
            If OSC_X>127
               OSC_X=OSC_X-256
            End If 
            OSC_Y=Peek(OSC_START+M_LENGTH5+PI)
            If OSC_Y>127
               OSC_Y=OSC_Y-255
            End If 
            Gosub DRW_OSC
            Inc PI : Inc PI
         Until PI>(BSIZE*2)
      End If 
   End If 
