Repeat 
   PCN=PI*PDV
   PCN=(Peek(OSC_START+PCN)*32)/256
   If PCN>15
      PCN=PCN-32
   Else 
      PCN=PCN-(PCN*2)
   End If 
   If PCN=0
      PCN=-1
   End If 
   If PCN<PCL
      PCL=PCN
   End If 
   Inc PI : Inc PI
   If M0DE<>1 and M0DE<>4
      If PI=4 and M0DE=4
         OSC_START=OSC_START+M_LENGTH5
      Else If PI=8
         OSC_START=OSC_START+M_LENGTH5
      Else If PI=12 and M0DE=4
         OSC_START=OSC_START+M_LENGTH5
      End If 
   End If 
Until PI>BSIZE/10
If PCL<=HPCL
   HPCL#=PCL
Else 
   HPCL#=HPCL#*0.8
End If 
PCL=Int(HPCL#)
Scroll 7
Cls _HIGHLIGHT,LEV_X,LEV_Y+PCL To LEV_X+15,LEV_Y
