HEADER:
Cls _PANE,0,0 To SCR_W,HDR_H
Ink _SHADOW,_PANE
T$=Str$(B)-" "
If Len(F$)<19
   Text 6,8,T$+":"+F$
Else 
   Text 6,8,T$+":"+Left$(F$,15)+"..."
End If 
MFREE=Fast Free+Chip Free
If MFREE>10000000
   MFREE=MFREE/1048576
   T$="M"
Else 
   MFREE=MFREE/1024
   T$="K"
End If 
Text SCR_W-83,8,"Free:"+Str$(MFREE)-" "+T$
HEADERCORDS:
Ink _SHADOW,_PANE
Text 244,8,Space$(9)
Text 244,8,Str$(Length(PBANK))
Text 210,8,"Size:"
Text 361,8,Space$(9)
Text 361,8,Str$(RST)
Text 319,8,"Start:"
Text 480,8,Space$(9)
Text 480,8,Str$(RL)
Text 438,8,"Range:"
