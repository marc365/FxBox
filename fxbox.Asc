   F$=Right$(MENB$(B),10)
   If N<11
      Right$(MENB$(B),10)=Right$(MENB$(N),10)
   Else 
      Right$(MENB$(B),10)=Left$(FB$(B),8)
   End If 
Else 
   F$=Left$(FB$(N),8)
End If 
If N<11
   Right$(MENB$(N),10)=F$
End If 
F$=FB$(B)
