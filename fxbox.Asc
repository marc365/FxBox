         If KEYC<>-1
            T0NEV#=KEYF/100.0
            Gosub T0SET
            ROT#(10)=(((T0NEV#-5)*4.8)/(FREQ/8))+8.5
            Gosub DIALT0NE
         End If 
      End If 
   Else If M=1
      Restore T0B
      I=1
      Repeat 
         Read BTXT1$,BX,BY,BZN,G$
         If Z=BZN
            Add BX,RX : Add BY,RY : Gosub PRESS
            Gosub _UNDOSET
            _REPEAT$=G$
            Gosub G$
            Gosub STEREOQUAD
            G$=_$
            Exit 
         End If 
         Inc I
      Until I>_TB
      If G$=_$
