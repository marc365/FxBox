   Else If C=76
      T0NEV#=T0NEV#+1
      Gosub T0SET
   Else If C=77
      T0NEV#=T0NEV#-1
      Gosub T0SET
   End If 
   If KDOWN and M=0
      BX=269 : BY=85 : Gosub HIGHLIGHT
      KDOWN=False
   End If 
Until A=27
Goto T0NEC
