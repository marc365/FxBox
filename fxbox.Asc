¯LOOP:
T$=_$ : TXT_X=0 : N=0
Print Dir$;"> ";_SCORE$;
Repeat 
   Gosub GVAL
   If C>0
      If A=30
         T$=_REPEAT$
         N=Len(_REPEAT$)
      Else 
         Gosub _TEXTINPUT
      End If 
      Locate Len(Dir$), : Print "> ";T$;Space$(2);
      If A<>13
         Writing 2,2
         Locate Len(Dir$)+2, : Print Space$(N);_SCORE$;
         Writing 0,0
      End If 
   End If 
Until A=13 or A=27
If A=27 Then Goto _DIRECTC
Print 
T$=Upper$(T$)
_REPEAT$=T$
_COMMAND:
PASS=0
Restore CMD
CI=1
Repeat 
   Read CMD$,CMDL,CMDG$
   If Left$(T$,CMDL)=CMD$
      Inc CMDL
      If Mid$(T$,CMDL-1,1)<>"="
         Inc CMDL
      End If 
      T$=Mid$(T$,CMDL)
      Gosub CMDG$
      CMDL=True
      Exit 
   End If 
   Inc CI
Until CI>25
If Not CMDL
   If MACRO
      Trap Gosub T$
      Gosub CHECKFX
      If MENFX
         MENFX=False
         G$=T$
         Gosub STEREOQUAD
      End If 
      If Errtrap
         E$=T$ : Gosub _MACROERROR
      End If 
      Return 
   Else 
      Screen Hide 2 : Screen 0
      Trap Gosub T$
      Gosub CHECKFX
      If MENFX
         MENFX=False
         G$=T$
         Gosub STEREOQUAD
      End If 
      If _REPEAT$="_INIT"
         Goto ¯EXIT
      End If 
      Screen Show 2 : Screen 2
      If Errtrap
         Print "Unknown command/subroutine: ";T$
      End If 
   End If 
End If 
If Not MACRO
   If Not _REPEATING
      Goto ¯LOOP
   End If 
End If 
Return 
