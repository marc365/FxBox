CHECKFX:
M1=1
MCOUNT=0
Repeat 
   M2=1
   Repeat 
      S$=Str$(M1)+Str$(M2)-_SPACE$
      Restore 0+Val(S$)
      Read S$
      If Instr(S$,"»")
         M3=1
         Repeat 
            S$=Str$(M1)+Str$(M2)+Str$(M3)-_SPACE$
            Restore 0+Val(S$)
            Read S$
            If S$<>_WHITESPACE$ and S$<>_NULL$
               Read MENFX,G$
               If G$=T$
                  Return 
               End If 
            End If 
            Inc M3
         Until S$=_NULL$
         S$=_$
      Else If Mid$(S$,2,1)<>_MINUS$ and S$<>_WHITESPACE$ and S$<>_NULL$
         Read MENFX,G$
         If G$=T$
            Return 
         End If 
      End If 
      Inc M2
   Until S$=_NULL$
   Inc M1
Until M1>_M1
MENFX=False
Return 
'
MACRO:
S$=FMACRO$(_M2SEL-1+MOFF)
MACROP:
_TOAST$=S$+_SPACE$
Gosub T0AST
D$=MACRODIR$+S$+".asc"
If Exist(D$)
   Gosub _UNDOSET
   MACRO=True : _TEXT=True
   _MODEP=_MODE
   Trap Open In 1,D$
   If Errtrap
      F$=D$ : Goto _FILEERROR
   End If 
   If Instr(_REPEAT$,S$)=False and _REPEATING=False and MI=1
      _REPEAT$=_REPEAT$+_SPACE$+S$
   End If 
   Inc MI
   MN(MI)=Lof(1) : Close 1 : MACROT$(MI)=Space$(MN(MI))
   Bload D$,Varptr(MACROT$(MI)) : MP(MI)=1
   _TIMEDIVP#=_TIMEDIV#
   Repeat 
      MA(MI)=Instr(MACROT$(MI),_NEWLINE$,MP(MI))
      If MA(MI)
         T$=Mid$(MACROT$(MI),MP(MI),MA(MI)-MP(MI))
         _TOAST$=T$+_SPACE$ : Gosub T0AST
         T$=Upper$(T$)
         Gosub _COMMAND
      End If 
      MP(MI)=MA(MI)+1
      Gosub GVAL
   Until A=27 or MA(MI)=False or MP(MI)>=MN(MI)
   T$=Str$(_TIMEDIVP#)
   Gosub TIMEDIV
   _MODE=_MODEP
   Dec MI
   If MI=1
      MACRO=False
      _TEXT=False
   End If 
Else 
   E$=D$ : Goto _MACROERROR
End If 
Return 
'
_MACROMENU:
MOFF=0
If Exist(MACRODIR$)
   Dir$=MACRODIR$
   MACRODIR$=Dir$
   D$=Dir First$(_$)
   TCOUNT=1
   If D$<>_$
      Repeat 
         I=Len(D$)-9
         Repeat 
            If Mid$(D$,I,1)<>_SPACE$
               Exit 
            End If 
            Dec I
         Until I<0
         D$=Upper$(Mid$(D$,2,1))+Mid$(D$,3,I-6)
         FMACRO$(TCOUNT)=D$
         Inc TCOUNT
         D$=Dir Next$
      Until D$=_$ or TCOUNT>_MR
      _MR=TCOUNT
   End If 
   TCOUNT=0
   Dir$=PROGDIR$
End If 
Goto DRWMACROMENU
'
MACRODOWN:
Inc MOFF
If MOFF>_MR-19 Then MOFF=_MR-19
Goto DRWMACROMENU
'
MACROUP:
Dec MOFF
If MOFF<0 Then MOFF=0
'
DRWMACROMENU:
I=0
Repeat 
   MENM$(I)=Space$(20)
   Mid$(MENM$(I),2)=FMACRO$(I+MOFF)
   Inc I
Until I>18
Return 
