   TCOUNT=1
   Repeat 
      If D$<>""
         For I=Len(D$)-9 To 0 Step -1
            TDC$=Mid$(D$,I,1)
            If TDC$<>" "
               TDFC=I
               Exit 
            End If 
         Next I
         D$=Upper$(Mid$(D$,2,1))+Mid$(D$,3,TDFC-2-4)
         MACROS$(TCOUNT)=D$
         _MENU$(7,TCOUNT,0)=Space$(20)
         Mid$(_MENU$(7,TCOUNT,0),2)=D$
         _MENUG$(7,TCOUNT,0)="MACRO"
         _MENUFX(7,TCOUNT,0)=False
