_MIX:
Gosub SET_PBANK
If FX_LENGTH<2 Then Goto FLSHSCR
FX_LENGTH2=Length(CBANK)
If FX_LENGTH2=0 Then Goto _NOCOPY
Gosub _UNDOSET
FX_START2=Start(CBANK)
Dec FX_LENGTH2
FX2=0
Repeat 
   CN=Peek(FX)
   CN2=Peek(FX_START2+FX2)
   Gosub SIGN2
   If CN<0 and CN2<0
      CN=(CN+CN2)-((CN*CN2)/-127)
   Else If CN>0 and CN2>0
      CN=(CN+CN2)-((CN*CN2)/127)
   Else 
      Add CN,CN2
   End If 
   Inc FX2
   If FX2>FX_LENGTH2
      If _LPMIX
         FX2=0
      Else 
         FX=FXE
      End If 
   End If 
   Gosub UNSIGN
   Poke FX,CN
   Inc FX
Until FX>FXE
Return 
