      M1=_M1
      Repeat 
         If CX>=MENX(M1-1) and CX<=MENX(M1)
            _M1SEL=M1
            Exit 
         End If 
         Dec M1
      Until M1<1
   End If 
   If M1>_M1 or M1=0
      M1=0
      Return 
   End If 
   If _PM1SEL<>_M1SEL
      Gosub CLRMENU
      _END=0
      SUBH=0
      MENH=0
   End If 
   _PM1SEL=_M1SEL
   _M2SEL=CY/8
   Restore 0+M1
   Read S$
   If _M2SEL>M2-2 or CX<MENX(M1-1) or CX>(MENX(M1-1)+(Len(_BAR$)*8))
      _M2SEL=0
   End If 
   _M3SEL=0
   _L1=(MENX(M1)/8)-Len(S$)
   MENX=(_L1*8)
   MENY=0
   M2=1
   Repeat 
      Locate _L1,M2
      If M1>0 and M2>0
         If M1=3 and M2=11 : Rem menu collision 3 11 vs 3 1 1   
            S$=_NULL$
         Else 
            Restore 0+Val(Str$(M1)-_SPACE$+Str$(M2)-_SPACE$)
            Read S$
         End If 
         If _M2SEL=M2 and Mid$(S$,2,1)<>_MINUS$
            Paper _PANE
            If Instr(S$,"»")
               _MENUSUB=True
            End If 
            If Instr(S$,"­") : Rem the other minus
               _MENUACTION=True
               Read MENFX,G$
               If _END>0
                  Gosub G$
                  Repeat 
                     Gosub TASK
                     Gosub MKEY
                  Until Timer>_END or M=0
               End If 
               Gosub SPINWAIT
               _END=Timer+8
            Else 
               _MENUACTION=False
               _END=0
            End If 
         Else If _M2SEL=0
            _END=0
         Else 
            Paper _SHADOW
         End If 
         Pen _BLACK
         Print S$
         If Len(S$)>1
            MENW=Text Length(S$)
         End If 
      End If 
      Inc M2
   Until S$=_NULL$
   MENH=(M2-1)*8
   If _MENUSUB=False and _MENUACTION=False
      Gosub TESTMOVE
   End If 
End If 
If Not _MENUON
   If OSCILLO
      RENDERING=True
      Gosub SH0WWAVE
      Gosub FRESHSEL
      RENDERING=False
   End If 
   Gosub CLRMENUBG
