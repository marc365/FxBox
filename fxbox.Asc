¯HELPSUBS:
Print "Macro Subroutines: ";
M1=1
MCOUNT=0
Repeat 
   M2=1
   Repeat 
      S$=Str$(M1)+Str$(M2)-_SPACE$
      Restore 0+Val(S$)
      Read S$
      If Instr(S$,"»")
         M3=1
         Repeat 
            S$=Str$(M1)+Str$(M2)+Str$(M3)-_SPACE$
            Restore 0+Val(S$)
            Read S$
            If S$<>_WHITESPACE$ and S$<>_NULL$
               Read MENFX,G$
               If Right$(G$,4)<>"MENU"
                  Print G$;_SPACE$;
               End If 
            End If 
            Inc M3
         Until S$=_NULL$
         S$=_$
      Else If Mid$(S$,2,1)<>_MINUS$ and S$<>_WHITESPACE$ and S$<>_NULL$
         Read MENFX,G$
         If Left$(G$,1)=_SCORE$ and Right$(G$,4)<>"MENU" and G$<>_MACRO$
            Print G$;_SPACE$;
         End If 
      End If 
      Inc M2
   Until S$=_NULL$
   Inc M1
Until M1>_M1
Print 
Return 
