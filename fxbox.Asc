                        End If 
                     Next M3
                  Else 
                     Paper _SHADOW
                  End If 
                  Print _MENU$(M1,M2,0)
               End If 
            End If 
         End If 
      Next M2
   End If 
   If Not _MENUON
      Cls _SHADOW,0,0 To SCR_W,HDR_H
