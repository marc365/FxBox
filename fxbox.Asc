MCHOOSE:
_CHOICE=False
If G$=_$ Then Return 
PASS=0
If RL=0 and MENFX
   If(_M1SEL=3 and(_M2SEL<>2 and _M2SEL<>7)) or _M1SEL<>3
      RL=P_LENGTH
      _CHOICE=3
   End If 
End If 
_REPEATP$=_REPEAT$
If G$="_UNDO"
   _REPEAT$=_REPEATP$
Else 
   _REPEAT$=G$
End If 
Gosub G$
If MENFX and RL>1
   Gosub STEREOQUAD
End If 
If MENFX
   If _CHOICE=3
      RL=0 : XR=0 : XR2=0
      _CHOICE=False
      Gosub POS_CAL
      Gosub HEADERCORDS
   End If 
   If REQ or FX=>FXE
      RENDERING=True
      Gosub _DRAW
      Gosub FRESHSEL
      Gosub RANGEPXCLEAR
      RENDERING=False
   End If 
End If 
If Right$(_REPEAT$,4)<>"MENU" and REQ
   _TOAST$=_REPEAT$
End If 
Return 
'
BCHOOSE:
PASS=0
If Z=58
   Goto _TGLOSCILLO
Else If Z=59
   Goto _TGLOSCILLOMODE
End If 
If Z<=_BU
   PZN=Z
   Gosub PRESSBUT
End If 
If Z=7 and PLYING and CX>56
   _TOAST$="_STOP"
   Goto _STOP
Else If Z=6
   _TOAST$="_PASTE"
   Goto _PASTE
Else If Z=11
   _TOAST$=_REPEAT$
   Goto _REPEAT
Else If P_LENGTH=0 and _MODE<>4
   Goto FLSHSCR
End If 
On Z-69 Goto LEFTWAND,RIGHTWAND,MOVEWAND,RANGE,SELLEFT,RANGE,SELRIGHT,RANGE
If Z<=_BU
   G$=BUTG$(Z)
   Gosub G$
   If Z=10 : Rem mix
      If RL>1
         Gosub STEREOQUAD
      End If 
      Gosub _DRAWALL
   End If 
   _TOAST$=G$
End If 
Return 
'
KCHOOSE:
PASS=0
If S=68 or S=64 or S=128 or S=132
   If C<5
      _M3SEL=C+1 : Goto M0DEMENU
   Else If C=16
      Goto _QUIT
   Else If C=19
      Goto _REVERT
   Else If C=24
      Goto _OPEN
   Else If C=32
      Goto _APPEND
   Else If C=33
      Goto _SAVEAS
   Else If C=34
      Goto _DIRECT
   Else If C=49
      Goto _UNDO
   Else If C=50
      Goto _CUT
   Else If C=51
      Goto _COPY
   Else If C=52
      Goto _PASTE
   End If 
Else If C=11
   Goto _PREVIOUS
Else If C=12
   Goto _NEXT
Else If C=74
   Add MUL,-10
   If MUL<0
      MUL=0
   End If 
   B=MUL
   If B<1
      B=1
   End If 
   C=B+79
   Goto FKEY
Else If C=94
   Add MUL,10
   If MUL<0
      MUL=0
   End If 
   B=MUL
   C=B+79
   Goto FKEY
Else If C=64
   Goto _STOP
Else If C=65 and PLYING
   If _MODE=4
      T$="_REWINDALL"
   Else 
      T$="_REWIND"
   End If 
   If SETTING$(26)=_FALSE$
      Goto T$
   Else 
      T$=T$+_COMMA$+_NEWLINE$
      Goto ACTION
   End If 
Else If C=68
   Goto _TGLLP
Else If C=69
   Goto _QUIT
Else If C>28 and C<32
   C=C+51+MUL
   Goto FKEY
Else If C>44 and C<48
   C=C+38+MUL
   Goto FKEY
Else If C>60 and C<64
   C=C+25+MUL
   Goto FKEY
Else If C>79 and C<90
   Goto FKEY
Else If C=122 or C=123
   Goto WHEELZOOM
Else If S<>%1000000
   If P_LENGTH=0
      Goto FLSHSCR
   End If 
   Gosub PIANO
   If KEYC<>-1
      _TOAST$=KEYS$
      FREQ=KEYF
      Gosub SETHZ
      Gosub DIALMAIN
      If RENDERING
         Gosub DRWHZ
         Gosub DRWTIMENOW
      End If 
      If RL>0 and RENDERING=False
         Gosub NUDGEPOS
      End If 
      If RL>0
         Goto _PLAYRANGE
      Else If DISPPLY
         Goto _PLAYDISPLAY
      Else 
         Goto _PLAYALL
      End If 
   End If 
End If 
Return 
'
ACTION:
N=Instr(SEQ$,_NEWLINE$)
If N=0
   SEQ$=T$
Else If N=Len(SEQ$)
   SEQ$=SEQ$+T$
Else 
   SEQ$=Left$(SEQ$,N-1)+T$+Mid$(SEQ$,N)
End If 
Return 
'
PIANO:
If S=4 Then Restore KLO Else Restore KHI
Repeat 
   Read KEYC,KEYS$,KEYF
   If KEYC=C
      Return 
   End If 
Until KEYC=-1
Return 
