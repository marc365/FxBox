Gosub _CLIOUT
Gosub _INIT
Do 
   Gosub GVAL
   
   If M=1
      Rem mouse zone does not allow 640  
      If CX=SCR_W and CY>HDR_H and CY<WVE_H+HDR_H
         Z=73
      End If 
      If Z>0
         Gosub BCHOOSE
      End If 
      Gosub DRWDIALS
   Else If M=2
      Gosub DRWMENU
   Else If _MENUON
      Gosub DRWMENUC
   Else If _CHOICE
      Gosub MCHOOSE
   Else If C>0
      Gosub KCHOOSE
   Else If PZ<>Z
      Gosub ZCHOOSE
   End If 
   '
   '********************
   If REXX
      If Arexx
         T$=Upper$(Arexx$(0))
         Arexx Answer 0,"ok"
         MACRO=True
         Gosub _COMMAND
         MACRO=False
      End If 
   End If 
   '********************
   '
   If Not PLYING
      If Not STPPED
         _COL=_PANE
         BZN=7 : Gosub ZBUTTON
         BZN=8 : Gosub ZBUTTON
         BZN=9 : Gosub ZBUTTON
         STPPED=True : BGPLYING=False : RNGEPLY=False : DISPPLY=False
         Gosub RANGEPXCLEAR
         Gosub GNOMEC
         Gosub BGREC
         Gosub CLEARPLAYBUFFER
      End If 
   End If 
