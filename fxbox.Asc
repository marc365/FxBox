_DIRECT:
_DIRECT=True
Screen Open 2,640,128,2,Hires
Palette $0,$FFF
Flash Off 
I#=-100.0 : DD#=1.0 : ACC#=1.5
Repeat 
   Screen Display 2,128,Int(I#),320,128
   If Not BGPLYING
      Multi Wait 
   End If 
   I#=I#+DD#
   DD#=DD#+ACC#
   ACC#=ACC#+0.5
Until I#>84+HDR_H
¯LOOP:
Print 
IN$="" : N=0
Print Dir$;"> ";
Repeat 
   Gosub GVAL
   If A>0
      If A=30
         IN$=PIN$
         N=PN
      Else 
         IN$=IN$+K$
         N=N+1
      End If 
      Locate 0, : Print Dir$;"> ";IN$;
   End If 
Until A=13 or A=27
IN$=Upper$(Mid$(IN$,0,N-1))
PIN$=IN$
PN=N-1
Print 
If Left$(IN$,4)="EXIT" or A=27
   Gosub _DIRECTC
   Return 
Else If Left$(IN$,2)="CD"
   Gosub ¯CD
Else If Left$(IN$,2)="MD"
   Gosub ¯MD
Else If Left$(IN$,3)="DIR"
   Gosub ¯DIR
Else If Left$(IN$,3)="DEL"
   Gosub ¯DEL
Else If Left$(IN$,3)="CLS"
   Cls 
Else If Left$(IN$,2)="RX"
   Gosub ¯RX
Else If Left$(IN$,9)="HELP SUBS"
   Gosub ¯HELPSUBS
Else If Left$(IN$,10)="HELP 14BIT"
   Gosub ¯HELP14
Else If Left$(IN$,4)="HELP"
   Gosub ¯HELP
Else 
   Screen Hide 2
   Screen 0
   Trap Gosub IN$
   Screen Show 2
   Screen 2
   If Errtrap
      Print "Unknown command/subroutine: ";IN$
   End If 
End If 
Goto ¯LOOP
