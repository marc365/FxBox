_TGLBGREC:
If BGREC
   BGREC=False
   Close 3
   If _MODE>1
      Close 4
      If _MODE>2
         Close 5 : Close 6
      End If 
   End If 
   _BGRECORD$=" Record Output   "
   Colour _HIGHLIGHT,_COLP
Else 
   If Length(PBANK)=0
      Goto FLSHSCR
   End If 
   PF$=F$
   If _MODE=2
      WTXT$="as Stereo"
   Else If _MODE=3
      WTXT$="as Quad"
   Else If _MODE=4
      WTXT$="4 Track"
   Else 
      WTXT$=_$
   End If 
   WTXT$="Record "+WTXT$+": raw"
   Gosub FREQUEST
   S$=F$
   F$=PF$
   If Not FRQ
      Return 
   End If 
   If Exist(S$)
      Gosub _OVERWRITE
      If Not REQ
         Return 
      End If 
   End If 
   If _MODE=1
      Open Out 3,S$
   Else If _MODE=2
      Open Out 3,S$+"-L"
      Open Out 4,S$+"-R"
   Else If _MODE=3 or _MODE=4
      Open Out 3,S$+"-1"
      Open Out 4,S$+"-2"
      Open Out 5,S$+"-3"
      Open Out 6,S$+"-4"
   End If 
   _BGRECORD$=" Stop Recording  "
   BGREC=True
   _COLP=Colour(_HIGHLIGHT)
   Colour _HIGHLIGHT,$F00
End If 
BGRECTOTAL=-1
Return 
