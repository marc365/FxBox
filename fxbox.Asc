'
_OPEN:
PB=B
_APPEND=B-1 : Rem always insert
WTXT$="Open"
Goto _OPENP
_OPENA:
WTXT$="Append"
_OPENP:
WTXT$=WTXT$+": raw iff aif wav abk med"
PF$=F$ : F$=_$
Gosub FREQUEST
If Not FRQ
   Swap B,PB
   If B<>PB
      Gosub SETBANK
      PF$=F$
   End If 
   F$=PF$
   Return 
End If 
_OPENF:
If Not Exist(F$)
   Gosub _CANTFIND
   F$=PF$
   Return 
End If 
Gosub _RANGENONE
_TEXT=True
If B=UB Then Gosub _CLEARUNDO
_CHANNELS=0 : _RATE=0 : _TAG=0 : _BITS=0
BANKNAME$=_$ : _ABK=False : _MMD=False
_IFF=False : _WAV=False : _FLOAT=False
Trap Open In 1,F$
If Errtrap Then Goto _FILEERROR
LF=Lof(1)
If LF=0
   F$=PF$
   Close 1
   Return 
End If 
Gosub SETFILENAME
FH$=Input$(1,4)
If FH$="FORM"
   _IFF=True
Else If FH$="RIFF"
   _WAV=True
Else If FH$="AmBk"
   _ABK=True
Else If Mid$(FH$,0,3)="MMD"
   _MMD=True
End If 
If _MMD
   BANKNAME$=F$
   F$=Space$(Len(F$))
   Gosub SETFILENAME
   S$=Input$(1,4) : PL=Leek(Varptr(S$)) : Rem length 
   S$=Input$(1,4) : P=Leek(Varptr(S$)) : Rem pointer
   Pof(1)=P+787
   S$=Input$(1,1) : NUMSAMPLES=Peek(Varptr(S$)) : Rem numsamples   
   NN=0 : N=B
   Pof(1)=32
   S$=Input$(1,4) : P=Leek(Varptr(S$)) : Rem exp pointer
   Pof(1)=P
   S$=Input$(1,20)
   S$=Input$(1,4) : NAMES=Leek(Varptr(S$))
   S$=Input$(1,2) : ENTRIES=Deek(Varptr(S$)) : Rem entries
   S$=Input$(1,2) : ENTSIZE=Deek(Varptr(S$)) : Rem enterz   
   Pof(1)=NAMES
   For I=1 To ENTRIES
      S$=Input$(1,ENTSIZE)
      P=Peek(Varptr(S$))
      If P>0
         For I2=1 To Text Length(S$)
            CN=Peek(Varptr(S$)+I2)
            If CN=0
               Exit 
            End If 
         Next I2
         FILES$(I)=Mid$(S$,0,I2)
      Else 
         FILES$(I)=_UNNAMED$
      End If 
   Next I
   Pof(1)=24
   S$=Input$(1,4) : P=Leek(Varptr(S$)) : Rem pointer to smplarr pointer 
   Pof(1)=P
   For I=1 To ENTRIES
      S$=Input$(1,4) : P=Leek(Varptr(S$)) : Rem pointer to InstrHdr pointers   
      If P>0 and P<Lof(1)-Pof(1)
         PNTERS(I)=P
      Else 
         PNTERS(I)=-1
      End If 
   Next I
   FX_START=0
   FX_LENGTH=Lof(1)
   For I=1 To ENTRIES
      B=N
      If PNTERS(I)>0
         Pof(1)=PNTERS(I)
         S$=Input$(1,4) : PL=Leek(Varptr(S$)) : Rem sample length
         S$=Input$(1,2) : T=Deek(Varptr(S$)) : Rem sample type
         If T=16
            T=0 : Rem 16bit mono 
         End If 
         If T=48
            T=0 : PL=PL*2 : Rem 16bit stereo 
         End If 
         If T=32
            T=0 : PL=PL*2 : Rem stereo 
         End If 
         If T=0 and PL>0
            Erase 7+N
            Trap Reserve As Work 7+N,PL
            If Errtrap
               Close 1
               F$=_$
               Goto _NOMEMORY
            End If 
            AC=Start(7+N)
            WR=0
            While WR<PL
               SL=Min( Fn BUFFER,PL-WR)
               S$=Input$(1,SL)
               Copy Varptr(S$),Varptr(S$)+SL To AC+WR
               WR=WR+SL
               FX=FX+SL
            Wend 
            F$=FILES$(I)
            Gosub SETFILENAME
            Rem odd size file
            If Instr(Str$(PL/2.0),".")>0
               If Instr(Str$(PL/2.0),"E+")<=0
                  S$=Input$(1,1)
               End If 
            End If 
            Inc N
            Inc NN
         End If 
      End If 
   Next I
   Close 1
   If _MODE<>4
      Bank Swap 5,8+_APPEND
   End If 
   FPOS=0 : FTOTAL=0 : Gosub CLRFILES
   B=1+_APPEND : PB=B : F$=FB$(B)
   TXT1$="Octamed bank loaded" : TXT2$="with"+Str$(NN)+" samples" : Gosub _NOTICE
Else If _ABK
   BANKNAME$=F$
   F$=Space$(Len(F$))
   Gosub SETFILENAME
   S$=Input$(1,2)
   S$=Input$(1,6)
   S$=Input$(1,8)
   If S$<>"Samples " or Eof(1)
      _ABK=False
      Close 1
      F$=PF$
      TXT1$="Not a sample bank" : TXT2$=_$ : Goto _ERROR
   End If 
   S$=Input$(1,2) : NN=Deek(Varptr(S$))
   If Eof(1)
      _ABK=False
      Close 1
      F$=PF$
      TXT1$="Empty sample bank" : TXT2$=_$ : Goto _ERROR
   End If 
   SF$=Input$(1,4) : SL=Leek(Varptr(SF$)) : If SL>6 : SF$=SF$+Input$(1,SL-6) : End If 
   FX=0 : FX_START=0 : FX_LENGTH=Lof(1) : FXE=FX_LENGTH
   For N=1 To NN
      If Leek(Varptr(SF$)+(N-1)*4)
         If Eof(1)
            TXT1$="Error loading"+Str$(N)+Str$(NN) : TXT2$="bank, continuing..." : Gosub _ERROR
            Exit 
         End If 
         S$=Input$(1,14)
         CHRP=8
         For CHR=1 To 7
            CHRP=Peek(Varptr(S$)+CHR)
            If CHRP=0
               Exit 
            End If 
         Next CHR
         F$=Left$(S$,CHR)
         B=N+_APPEND : Gosub SETFILENAME
         SZ=Leek(Varptr(S$)+10)
         If SZ mod 2=1
            SZ=SZ+1
         End If 
         If SZ>0
            Erase 7+N+_APPEND
            Trap Reserve As Work 7+N+_APPEND,SZ
            If Errtrap
               Close 1
               F$=_$
               Goto _NOMEMORY
            End If 
            AC=Start(7+N+_APPEND)
            P=0
            While P<SZ
               SL=Min( Fn BUFFER,SZ-P)
               If Not Eof(1)
                  S$=Input$(1,SL)
                  Copy Varptr(S$),Varptr(S$)+SL To AC+P
                  P=P+SL
                  FX=FX+SL
               Else 
                  P=SZ
               End If 
            Wend 
         End If 
      End If 
   Next 
   FX=FXE
   Close 1
   If _MODE<>4
      Bank Swap 5,8+_APPEND
   End If 
   B=1+_APPEND : PB=B : F$=FB$(B)
   TXT1$="Sample Bank loaded" : TXT2$="with"+Str$(NN)+" samples" : Gosub _NOTICE
Else 
   If PBANK>5 and _APPEND>0
      PBANK=_APPEND+8
   End If 
   Erase PBANK
   Trap Reserve As Work PBANK,1024
   If Errtrap
      Close 1
      F$=_$
      Goto _NOMEMORY
   End If 
   Pof(1)=0
   S$=Input$(1,1024)
   Copy Varptr(S$),Varptr(S$)+1024 To Start(PBANK)
   Close 1
   S_START=Start(PBANK)
   Gosub PARSEHEADER
   S_START=S_START-Start(PBANK)
   S_LENGTH=LF-S_START
   Erase PBANK
   Trap Reserve As Work PBANK,LF
   If Errtrap
      F$=_$
      Goto _NOMEMORY
   End If 
   Bload F$,PBANK
   Trap Reserve As Work WBANK,S_LENGTH
   If Errtrap
      F$=_$
      Goto _NOMEMORY
   Else 
      Copy Start(PBANK)+S_START,Start(PBANK)+S_START+S_LENGTH To Start(WBANK)
      Bank Swap PBANK,WBANK
      Gosub _CLEARWORK
      B=1+_APPEND : PB=1+_APPEND : F$=FB$(1+_APPEND)
   End If 
End If 
S$=_$
If _BITS>0 Then _TOAST$=Str$(_BITS)-_SPACE$+"bit "+_TOAST$
PMACRO=MACRO
MACRO=True
If _IFF and _BITS>8
   Gosub _STOP
   If _BITS=16
      Gosub _16BITTO8BIT
   Else If _BITS=24
      Gosub _24BITTO8BIT
   Else If _BITS=32 and _FLOAT
      Gosub _32BITTO8BIT
   End If 
Else If _WAV
   Gosub _STOP
   If _BITS=8
      Gosub _8BITUNSIGNEDTO8BIT
   Else If _BITS=16
      Gosub _16BITTO8BITFLIP
   Else If _BITS=24
      Gosub _24BITTO8BITFLIP
   Else If _BITS=32 and _FLOAT
      Gosub _32BITTO8BITFLIP
   Else If _TAG=0
      Gosub _NOTDECODED
   End If 
End If 
_MODEP=_MODE
If _CHANNELS>1
   Gosub _DEINTERLEAVE
   If _CHANNELS>2
      Gosub _DEINTERLEAVE
      If _MODE<>4
         _MODE=3
      End If 
   Else If _MODE<>4
      _MODE=2
   End If 
Else If _ABK and _APPEND=0
   _MODE=4
   Gosub R4INSET
End If 
